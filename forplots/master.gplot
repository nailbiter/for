set terminal png transparent nocrop enhanced size 450,320 font "arial,8" 
set output 'simple.1.png'
set samples 50, 50
set title "Simple Plots" 
#set vertical Right noreverse enhanced autotitle box lt black linewidth 1.000 dashtype solid
set key off
set samples 50, 50
set style line 5 lt rgb "cyan" lw 3 pt 6

p = 1
q = 5
n = p+q

plot [-10:10] x+2,x+4,x,n-1-x,n-1-x-2,n-1-x-4,1,3,5,0,-2,-4 lt 5
