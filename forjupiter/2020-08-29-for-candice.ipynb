   "source": [
    "from jinja2 import Template\n",
    "\n",
    "greeting = Template(\"\"\"\n",
    "{%for guest in guests-%}\n",
    "Dear {{guest}}, welcome to our wedding!\n",
    "{%endfor-%}\n",
    "\"\"\")\n",
    "\n",
    "def render_html(t,**kwargs):\n",
    "    return t.render(kwargs)\n",
    "\n",
    "guests = [\"Mike\",\"Candice\",\"Mariia\",\"泰国女生\"]\n",
    "\n",
    "print( render_html(greeting, guests=guests) )"
   ]
  }
